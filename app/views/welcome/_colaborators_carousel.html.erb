<div id="carousel-colaborators" class="carousel slide" data-ride="carousel">
  <!-- Indicators -->
  <ol class="carousel-indicators">
    <li data-target="#carousel-colaborators" data-slide-to="0" class="active"></li>
    <li data-target="#carousel-colaborators" data-slide-to="1"></li>
    <li data-target="#carousel-colaborators" data-slide-to="2"></li>
  </ol>

  <!-- Wrapper for slides -->
  <div class="carousel-inner" role="listbox">
    <div class="item active">
      <%= image_tag "logo_1.jpg", style: "width: 100%;" %>
    </div>
    <div class="item">
      <%= image_tag "logo_2.jpg", style: "width: 100%;" %>
    </div>
    <div class="item">
      <%= image_tag "logo_3.jpg", style: "width: 100%;" %>
    </div>

  </div>
</div>
